<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.meivaldi.trencenter">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature
        android:name="android.hardware.camera.any"
        android:required="true" />
    <uses-feature
        android:name="android.hardware.camera.autofocus"
        android:required="false" />

    <application
        android:name=".app.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.relawan.MainActivity" />
        <activity android:name=".activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.QR_Login" />
        <activity android:name=".activity.super_admin.Dashboard_SuperAdmin" />
        <activity android:name=".activity.ChangeUsername" />
        <activity android:name=".activity.ChangePassword" />
        <activity android:name=".activity.ProgramKerja" />
        <activity android:name=".activity.DetailProgram" />
        <activity android:name=".activity.relawan.InputRelawan" />
        <activity android:name=".activity.pendukung.InputPendukung" />
        <activity android:name=".activity.tim_pemenangan.Tim_Pemenangan"></activity>
    </application>

</manifest>